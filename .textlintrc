{
    "filters": {
        "allowlist": {
            "allow": [
                "ignored-word",
                "/\\d{4}-\\d{2}-\\d{2}/",
                "/===IGNORE===[\\s\\S]*?===\/IGNORE===/m"
            ]
        }
    },
  "rules": {
        "@textlint-rule/no-unmatched-pair": true,
        "no-nfd": true,
        "ja-no-abusage": true,
        "no-kangxi-radicals": true,
        "ja-no-successive-word": {
            allowOnomatopee: true,
        },
        "ja-unnatural-alphabet": true,
  },
  "plugins": [
    "html"
  ]
}